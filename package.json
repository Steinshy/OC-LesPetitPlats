{
  "name": "lespetitplats",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "npm run lint:eslint && npm run lint:css && npm run lint:html && npm run lint:md",
    "lint:eslint": "eslint . --ext js",
    "lint:css": "stylelint '**/*.css' --ignore-pattern '**/*.min.css'",
    "lint:html": "html-validate '**/*.html'",
    "lint:md": "markdownlint '**/*.md' --ignore node_modules",
    "lint:fix": "eslint . --ext js --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "analyze": "vite build --mode analyze",
    "optimize:images": "node scripts/optimizeImages.js",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.7",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^1.0.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^16.4.0",
    "html-validate": "^9.0.0",
    "jsdom": "^27.0.1",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.33.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "vite": "^7.1.7",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-linter": "^3.1.0",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-webfont-dl": "^3.11.1",
    "vitest": "^3.2.4"
  }
}
